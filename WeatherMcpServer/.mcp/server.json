{
  "$schema": "https://modelcontextprotocol.io/schemas/draft/2025-07-09/server.json",
  "description": "",
  "name": "io.github.OKtol/dotnet-test-assignment",
  "packages": [
    {
      "registry_name": "nuget",
      "name": "OKtol.WeatherMcpServer",
      "version": "0.1.1-beta",
      "package_arguments": [],
      "environment_variables": [
        {
          "name": "OpenWeatherOptions__BaseUrl",
          "value": "https://api.openweathermap.org/"
        },
        {
          "name": "OpenWeatherOptions__ApiKey",
          "value": "{openweather_api_key}",
          "variables": {
            "openweather_api_key": {
              "description": "Api key from your https://api.openweathermap.org/ account.",
              "is_required": true,
              "is_secret": false
            }
          }
        }
      ]
    }
  ],
  "repository": {
    "url": "https://github.com/OKtol/dotnet-test-assignment",
    "source": "github"
  },
  "version_detail": {
    "version": "0.1.1-beta"
  }
}
